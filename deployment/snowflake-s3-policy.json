{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AllowSnowflakeS3Access",
            "Effect": "Allow",
            "Action": [
                "s3:GetObject",
                "s3:GetObjectVersion",
                "s3:ListBucket",
                "s3:GetBucketLocation"
            ],
            "Resource": [
                "arn:aws:s3:::fin-trade-craft-landing",
                "arn:aws:s3:::fin-trade-craft-landing/*"
            ]
        },
        {
            "Sid": "AllowSnowflakeListAllBuckets",
            "Effect": "Allow",
            "Action": [
                "s3:ListAllMyBuckets"
            ],
            "Resource": "*"
        }
    ]
}