{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AllowSnowflakeAssumeRole",
            "Effect": "Allow",
            "Principal": {
                "AWS": "arn:aws:iam::SNOWFLAKE_ACCOUNT_ID:root"
            },
            "Action": "sts:AssumeRole",
            "Condition": {
                "StringEquals": {
                    "sts:ExternalId": "SNOWFLAKE_EXTERNAL_ID"
                }
            }
        }
    ]
}